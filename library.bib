number = {24},
pages = {15794--15802},
publisher = {Elsevier Ltd},
title = {{Toward low-cost Pd/ceramic composite membranes for hydrogen separation: A case study on reuse of the recycled porous Al2O3 substrates in membrane fabrication}},

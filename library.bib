keywords = {CO,Fossil fuel,Hydrogen,Low-carbon,Nuclear,Reforming,Solar},
pages = {489--522},
publisher = {Woodhead Publishing},
title = {{17 - Low-carbon production of hydrogen from fossil fuels}},
url = {http://www.sciencedirect.com/science/article/pii/B9781782423614000170},
year = {2015}
}

journal = {Molecules},
keywords = {Inorganic membranes,Membrane reactor,Membranes,Palladium membranes,Pore-filled},
number = {1},
pages = {1--53},
pmid = {28045434},
title = {{Recent advances in pd-based membranes for membrane reactors}},
volume = {22},
year = {2017}
}

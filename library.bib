issn = {19327447},
journal = {Journal of Physical Chemistry C},
number = {29},
pages = {15313--15321},
title = {{Gas adsorption characteristics of metal-organic frameworks via quartz crystal microbalance techniques}},
volume = {116},
year = {2012}
}

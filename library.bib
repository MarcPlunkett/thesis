issn = {1359-7345},
journal = {Chem. Commun.},
number = {12},
pages = {2280--2282},
pmid = {25558479},
publisher = {Royal Society of Chemistry},
title = {{Tetrazine functionalized zirconium MOF as an optical sensor for oxidizing gases}},

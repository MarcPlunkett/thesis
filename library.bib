keywords = {Carbon,Catalytic,Cracking,Decarbonization,Decomposition,Dehydrogenation,Dissociation,Engine,Gasoline,Hydrocarbons,Hydrogen,Methane,Plasma,Pyrolysis,Review,Thermo-catalytic},
number = {1},
pages = {1--24},
title = {{Decomposition of hydrocarbons to hydrogen and carbon}},
url = {http://www.sciencedirect.com/science/article/pii/S0926860X09001719},
volume = {359},
year = {2009}
}

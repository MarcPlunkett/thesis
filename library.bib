journal = {The Journal of Physical Chemistry B},
number = {39},
pages = {11352--11358},
pmid = {21875104},
title = {{Molecular Simulation of Gas Adsorption, Diffusion, and Permeation in Hydrated Nafion Membranes}},
url = {http://pubs.acs.org/doi/abs/10.1021/jp204141b},
volume = {115},
year = {2011}
}
